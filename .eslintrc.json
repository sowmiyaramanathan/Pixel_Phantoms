{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourcetype": "script"
    },
    "rules": {
        "no-unused-vars": "warn",
        "no-console": "off",
        "eqeqeq": "warn"
    }
}